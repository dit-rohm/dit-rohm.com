<ifModule mod_expires.c>
  ExpiresActive On
  ExpiresDefault "access plus 1 month"
  ExpiresByType text/html "access plus 30 minutes"
</ifModule>

<ifModule mod_deflate.c>
  AddOutputFilterByType DEFLATE text/html text/css application/javascript application/x-font-ttf application/vnd.ms-fontobject application/x-font-otf image/svg+xml text/plain
</ifModule>
